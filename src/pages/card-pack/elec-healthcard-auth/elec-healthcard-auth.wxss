.health-card-auth{
  padding: 40rpx;
}
.health-card-content {
  border-radius: 50rpx;
  background-repeat: no-repeat;
  background-image: url('https://bkyz-applets-1252354869.cos.ap-guangzhou.myqcloud.com/applets-imgs/healthAuthBac.png');
  height: 622rpx; 
  background-position: center;
  background-size: 100%;
  margin-top: 36rpx;
}
.health-card-content-info {
  padding: 40rpx;
  box-shadow: 0px 2px 10px 0px rgba(0,0,0,0.10);
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  box-sizing: border-box;

  
}
.health-card-content-info-txt{
  font-size: 28rpx;
  font-weight: light;
  text-align: left;
  color: #333333;
}
.health-card-content-info-dot{
  border-radius: 50%;
  height: 15rpx;
  width: 15rpx;
  margin-right: 20rpx;
  background-color: #56A1F4;
}
.health-card-content-info-subtxt{
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Light;
  font-weight: light;
  text-align: left;
  color:#999999;
}
.health-card-content-title{
  font-size: 36rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: bold;
  color: #ffffff;
  height: 350rpx;
  line-height: 520rpx;
  text-align: center;
}
.health-card-btns{
  padding: 60rpx 0;
}
.health-card-btn {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 34rpx;
  border-radius: 50rpx;

}
.primary {
  background-color: #56A1F4;
  color: white;
}
.cancel {
  background-color: #eeeeee;
  color: #999999;
  margin-top: 40rpx;
}